<div>
  <%= form_for [:admin, @post] do |f| %>
    <div class="">
      <%= f.label :title %>
      <%= f.text_field :title %>
    </div>

    <div class="">
      <%= f.label :body %>
      <%= f.text_area :body %>
    </div>

    <div class="">
      <%= f.submit %>
    </div>

  <% end %>
</div>
